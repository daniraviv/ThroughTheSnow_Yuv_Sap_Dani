<MudDialog Class="dialogDesign">

    <DialogActions>
      
        <MudButton Class="buttonGame2" OnClick="Cancel">לא, בטל</MudButton>
        <MudButton Class="buttonGame2" Color="Color.Primary" OnClick="Submit">כן, מחק</MudButton>
    </DialogActions>
</MudDialog>
@code {
    [CascadingParameter] MudDialogInstance MudDialog { get; set; }

    [Parameter]
    public string contentText { get; set; }


    void Submit() => MudDialog.Close(DialogResult.Ok(true));
    void Cancel() => MudDialog.Cancel();


}